{
  "name": "enginer",
  "version": "0.0.1",
  "description": "An utility js/ts library to make Javscript conccurent",
  "author": "Jo√£o Augusto Perin de Souza <joaoaugustoperin@gmail.com> (https://github.com/joojscript)",
  "license": "MIT",
  "keywords": [
    "concurrency",
    "javascript",
    "typescript",
    "worker",
    "threads",
    "pool"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "nodemon -r tsconfig-paths/register --watch 'src/' --exec \"ts-node src/index.ts\" -e ts",
    "test": "jest"
  },
  "source": "src/index",
  "files": [
    "dist/"
  ],
  "repository": "https://github.com/joojscript/enginer",
  "bugs": {
    "url": "https://github.com/joojscript/enginer/issues"
  },
  "homepage": "https://github.com/joojscript/enginer#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.13",
    "@types/worker-threads-pool": "^2.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "worker-threads-pool": "^2.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "../tests/sample"
    ]
  }
}
